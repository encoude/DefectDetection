   "source": [
    "import sympy as sp\n",
    "x,y = sp.symbols('x y')\n",
    "u = sp.Function('u')(x, y)\n",
    "pde = sp.Eq(x * sp.diff(u,x) - 3 * y * sp.diff(u,y),2 * x**2 * u)\n",
    "solution = sp.pdsolve(pde)"
   ]
  }
